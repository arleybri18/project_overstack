<!DOCTYPE html>
<html>

<head>
  <title>FlowOverstack</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>

  <% if signed_in? %>
  <div>Hola
    <%= current_user.name %>
  </div>
  <% end %>

  <%= yield %>
</body>

</html>
